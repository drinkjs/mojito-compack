{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2016",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution":"Node",
    "declaration": true,
    "outDir": "tsc-build",
    "baseUrl": "./",
    "resolveJsonModule":true,
    "sourceMap": true
  },
  "include": [
    "src",
  ]
}
